        padding: 0;
      }
      table {
        border-spacing: 0;
        border-collapse: collapse;
      }
      td {
        padding: 0;
      }
      a {
        text-decoration: none;
      }
      img {
        border: 0;
      }
      /* ======> Mobile <====== */
      @media only screen and (max-width: 599.8px) {
        .prod-sec-two-pad > table > tbody > tr > td {
          padding-top: 0 !important;
        }
        .prod-img > table > tbody > tr > td > a > img {
          width: 100% !important;
        }
      }
    </mj-style>
